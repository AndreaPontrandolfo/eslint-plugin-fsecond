{
  "name": "eslint-plugin-fsecond",
  "description": "ESLint plugin with misc rules.",
  "license": "MIT",
  "version": "0.0.0-development",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "format": "prettier --write .",
    "docs:update": "pnpm build && eslint-doc-generator && pnpm format",
    "docs:lint": "pnpm eslint-doc-generator --check",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc",
    "lint": "eslint --max-warnings=0",
    "knip": "knip",
    "qa": "pnpm build && pnpm typecheck && pnpm lint && pnpm test && pnpm knip && docs:lint",
    "semantic-release": "pnpm build && semantic-release"
  },
  "author": {
    "name": "Andrea Pontrandolfo",
    "email": "andrea.pontra@gmail.com",
    "url": "https://github.com/AndreaPontrandolfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreaPontrandolfo/eslint-plugin-fsecond.git"
  },
  "bug": "https://github.com/AndreaPontrandolfo/eslint-plugin-fsecond/issues",
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "react",
    "reactjs"
  ],
  "peerDependencies": {
    "eslint": ">=8.23.0",
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.18.2",
  "files": [
    "README.md",
    "package.json",
    "dist",
    "docs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.46.0",
    "@typescript-eslint/utils": "^8.46.0",
    "minimatch": "^10.0.3",
    "read-package-up": "^11.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "eslint-config-sheriff": "^29.0.0",
    "eslint-doc-generator": "^2.3.0",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-vitest-rule-tester": "^2.2.2",
    "jiti": "^2.6.1",
    "knip": "^5.65.0",
    "prettier": "^3.0.1",
    "publint": "^0.3.14",
    "semantic-release": "^25.0.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
