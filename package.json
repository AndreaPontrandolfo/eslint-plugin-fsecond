{
  "name": "eslint-plugin-fsecond",
  "description": "ESLint plugin with some opinionated and stylistic rules.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "vitest",
    "lint": "eslint --max-warnings=0",
    "semantic-release": "pnpm build && semantic-release"
  },
  "author": {
    "name": "Andrea Pontrandolfo",
    "email": "andrea.pontra@gmail.com",
    "url": "https://github.com/AndreaPontrandolfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreaPontrandolfo/eslint-plugin-fsecond.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.31.0",
    "@typescript-eslint/utils": "^8.31.0",
    "minimatch": "^10.0.1",
    "read-package-up": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/rule-tester": "^8.31.0",
    "@typescript-eslint/types": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-sheriff": "^27.1.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "semantic-release": "^24.2.3",
    "tsdown": "^0.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "version": "0.0.0-development",
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
